  "bugs": {
    "url": "https://github.com/VasilevVitalii/vv-mssql/issues"
  },
  "homepage": "https://github.com/VasilevVitalii/vv-mssql#readme",
  "devDependencies": {
    "jsdoc-to-markdown": "^5.0.3",
    "pre-commit": "^1.2.2"
  },
  "dependencies": {
    "tedious": "^8.3.0",
    "vv-mssql-shared": "0.0.2",
    "vv-shared": "^2.0.23"
  }
}
